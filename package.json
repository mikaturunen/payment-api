{
  "name": "node-type-seed",
  "version": "0.0.1",
  "description": "Tasker project",
  "main": "build/app.js",
  "scripts": {
    "build": "npm run compile && npm run copy-static",
    "compile": "./node_modules/typescript/bin/tsc -p tsconfig.json",
    "copy-static": "./node_modules/cpx/bin/index.js --clean --verbose 'src/**/*.{yaml,json}' build ",
    "start": "node ./build/app.js"
  },
  "author": "Mika Turunen",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.12",
    "copy-webpack-plugin": "^4.0.1",
    "express": "^4.15.3",
    "locutus": "^2.0.9",
    "ramda": "^0.24.1",
    "swagger-express-mw": "^0.7.0",
    "unirest": "^0.5.1",
    "xml2js": "^0.4.17",
    "yaml-loader": "^0.5.0",
    "typescript": "^2.3.4",
    "cpx": "^1.5.0",
    "@types/body-parser": "^1.16.3",
    "@types/bunyan": "^1.8.0",
    "@types/express": "^4.0.36",
    "@types/express-session": "^1.15.0",
    "@types/node": "^7.0.29",
    "@types/serve-static": "^1.7.31",
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "mocha": "^3.4.2"
  }
}
